{
	"name": "innologi/typo3-fileman",
	"type": "typo3-cms-extension",
	"description": "File manager for category-based uploading, viewing and managing files on frontend. Includes complete management in backend.",
	"license": "GPL-2.0+",
	"authors": [{
			"name": "Frenck Lutke",
			"email": "typo3@innologi.nl",
			"homepage": "http://www.innologi.nl",
			"role": "Developer"
		}
	],
	"require": {
		"typo3/cms-core": "^8.7",
		"php": "^7.1"
	},
	"autoload": {
		"psr-4": {
			"Innologi\\Fileman\\": "Classes/"
		}
	},
	"replace": {
		"fileman": "self.version",
		"typo3-ter/fileman": "self.version"
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	}
}